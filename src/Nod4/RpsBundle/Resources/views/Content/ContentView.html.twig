{% extends 'RpsBundle::layout.html.twig' %}
{% block title %}
    {{content.sname}}
    {% endblock %}
{% block entity_content %}

        {#<div class="col-sm-2" style="background-color: #E5E5E5;">#}
    <div class="col-sm-2 label " >
            <h4 class="bg-primary">{{content.dateUpdate | date('d-m-Y') }} </h4>
        </div>
        <div class="col-sm-10">
            <h3 style="margin-top: 0;">{{content.fname}}</h3>
        </div>
        


     <div class="col-sm-12">
    <div class="news_content_text lead">
        {% autoescape false %}
        <p>{{content.ftext}}</p>
        {% endautoescape %}
    </div>
     </div>

<div class="row">
    {%for image in content.images if content.images != null %}
    <div class="col-sm-offset-1 col-sm-10">
    <img src="{{image.getWebPath}}" style="margin-bottom:20px">
    </div>
    {%endfor%}
</div>





{% endblock %}